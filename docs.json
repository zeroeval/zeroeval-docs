{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "ZeroEval Documentation",
  "colors": {
    "primary": "#a3fca2",
    "light": "#07C983",
    "dark": "#0a0a0a"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Tracing",
            "pages": ["tracing/quickstart", "tracing/best-practices"]
          },
          {
            "group": "Evaluations",
            "pages": ["evaluations/coming-soon"]
          },
          {
            "group": "A/B Testing",
            "pages": ["ab-testing/coming-soon"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Platform",
          "href": "https://zeroeval.com",
          "icon": "code-simple"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/MuExkGMNVz",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:founders@zeroeval.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://zeroeval.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/LlmStats",
      "github": "https://github.com/zeroeval"
    }
  }
